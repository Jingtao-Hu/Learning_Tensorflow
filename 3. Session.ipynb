    "import tensorflow as tf\n",
    "# Two methods to create a session\n",
    "\n",
    "# First method\n",
    "sess = tf.Session()\n",
    "sess.run()\n",
    "sess.close()\n",
    "\n",
    "# Second method\n",
    "with tf.Session() as sess:\n",
    "    sess.run()\n",
    "    \n",
    "#  A function for interactive environment\n",
    "sess = tf.InteractiveSession()"
